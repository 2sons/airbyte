{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "searchIssues": {
      "type": "object",
      "properties": {
        "totalCount":{
          "type": "string"
        },
        "nodes": {
          "type": "array",
          "items": {
            "required": ["id", "ghId", "title", "body", "state", "createdAt", "updatedAt", "pipelineIssue"]
            , "properties": {
                "id": { "type": "string"}
                ,"ghId": { "type": "integer"}
                ,"title": { "type": "string"}
                ,"body": { "type": "string"}
                ,"state": { "type": "string"}
                ,"createdAt": {"type": "string", "format": "date-time", "airbyte_type": "timestamp_without_timezone"}
                ,"updatedAt": {"type": "string", "format": "date-time", "airbyte_type": "timestamp_without_timezone"}
                ,"pipelineIssue": { 
                    "type": "object"
                    ,"properties": {
                        "pipeline": {
                            "type": "object"
                            ,"properties": {
                                "id": { "type": "string"}
                                , "name": { "type": "string"}
                             }
                         }
                     }
                 }
             }
           }
        }
      }
    }
  }
}